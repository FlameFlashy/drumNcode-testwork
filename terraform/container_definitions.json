[
    {
        "name": "laravel",
        "image": "637423237571.dkr.ecr.eu-central-1.amazonaws.com/flameflashy-drumncode:laravel-dnc",
        "portMappings": [
            {
                "containerPort": 9000,
                "hostPort": 9000,
                "protocol": "tcp"
            }
        ],
        "essential": true,
        "environment": [
            {
                "name": "DB_NAME",
                "value": "drumncode_db"
            },
            {
                "name": "DB_USER",
                "value": "drumncode_user"
            },
            {
                "name": "DB_ENDPOINT",
                "value": "aws_db_instance.database.endpoint"
            },
            {
                "name": "DB_PASSWORD",
                "value": "drumncode_12321fds"
            }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "/ecs/laravel",
                "awslogs-region": "eu-central-1",
                "awslogs-stream-prefix": "ecs"
            }
        }
    }
]